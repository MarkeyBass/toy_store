<html lang="en">

  <%- include("partials/head.ejs") %>
  <body>
    <div class="container">
      <%- include("partials/products_nav.ejs") %>
      <form id='form' class="mt-4" action="/products" method="POST">
        <div class="form-group mb-4">
          <input type="number" id="part_number" placeholder="part_number" name="part_number" class="form-control">
        </div>
        <div class="form-group mb-4">
          <input type="text" class="form-control" id="description" name="description" placeholder="description">
        </div>
        <div class="form-group mb-4">
          <input type="number" class="form-control" id="qty" name="qty" placeholder="Quantity">
        </div>
        <div class="form-group mb-4">
          <input type="number" class="form-control" id="price" name="price" placeholder="price">
        </div>
        <div class="form-group mb-4">
          <input type="text" class="form-control" id="img_url" name="img_url" placeholder="img_url">
        </div>
        <input type="submit" class="btn btn-primary mb-4" value="submit">
      </form>
    </div>
  </body>

</html>